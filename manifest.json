{
  "manifest_version": 3,
  "name": "Cookie Injector",
  "version": "1.0.0",
  "description": "Inject cookies easily. Built by Rishad Alamy Tools.",
  "permissions": [
    "cookies",
    "tabs",
    "activeTab",
    "storage"
  ],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
